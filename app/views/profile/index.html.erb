<h3>Profile</h3>
<br/>
<table class="table">
	<thead>
		<tr>
			<th>Study Name</th>
			<th>Study Creator</th>
			<th>Default Study?</th>
		</tr>
	<thead>
	<tbody>
		<% @acls.each do |acl| %>
			<% if acl.study_is_active || acl.user_id == acl.study_user_id %>
				<td><%= link_to acl.study_title, study_dashboard_index_path(acl.study.id) %></td>
				<% if acl.user_id != acl.study_user_id %>
					<td><%= acl.study_email %></td>
				<% else %>
					<td>You</td>
				<% end %>
				<td></td>
			</tr>
			<% end %>
		<% end %>
	</tbody>
</table>		
