<table class="table table-bordered table-striped">
	<thead>
		<tr>
			<th>Form Name</th>
			<th>Link To Builder</th>
			<th>Creator</th>
			<th>Data Entry</th>
		</tr>
	</thead>
	<tbody>
		<% @forms.each do |form| %>
			<tr>
				<td><%= form.name %></td>
				<td><%= link_to "Form Builder", generate_form_builder_link(form.id), :class => "btn btn-info" %></td>
				<td><%= form.user.email %></td>
				<td>
					<% if form.is_published %>
						<%= link_to "Entry", study_form_url(form.study,form), :class => "btn btn-info" %>
					<% end %>
				</td>
			</tr>
		<% end %>
	</tbody>
</table>